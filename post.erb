	<item>
		<title><%= @post_title %></title>
		<link><%= @post_link %></link>
		<dc:creator>evanweaver</dc:creator>
		<guid isPermaLink="false">http://blog.evanweaver.com/?p=<%= @post_id %></guid>
		<description></description>
		<content:encoded><![CDATA[<%= @post_contents %>]]></content:encoded>
		<% if @post_excerpt %>
		  <excerpt:encoded><![CDATA[<%= @post_excerpt %>]]></excerpt:encoded>
		<% end %>
		<wp:post_id><%= @post_id %></wp:post_id>
		<wp:post_date_gmt><%= @post_date_gmt %></wp:post_date_gmt>
		<wp:comment_status>closed</wp:comment_status>
		<wp:ping_status>closed</wp:ping_status>
		<wp:post_name><%= @post_short_link %></wp:post_name>
		<wp:status>publish</wp:status>
		<wp:post_parent>0</wp:post_parent>
		<wp:menu_order>0</wp:menu_order>
		<wp:post_type>post</wp:post_type>
		<wp:post_password></wp:post_password>
		<wp:is_sticky>0</wp:is_sticky>
		<category domain="category" nicename="uncategorized"><![CDATA[Uncategorized]]></category>
    	<%= @post_comments %>
	</item>
